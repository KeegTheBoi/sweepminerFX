<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox fx:id="mainPane" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="300.0" minWidth="400.0" spacing="30.0" styleClass="pane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafxgrid.view.SettingsView">
   <children>
      <Label fx:id="lblDifficulty" alignment="CENTER">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmbDifficulty" prefWidth="150.0" />
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#goToMainMenu" text="Main Menu" />
   </children>
</VBox>
